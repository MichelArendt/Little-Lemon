{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "allowJs": true,
      "checkJs": false,
      "jsx": "react",
      "noEmit": true,
      "baseUrl": "./src",
      "paths": {
        "@/*": ["src/*"]
      }
    },
    "include": ["src"],
    "resolvedPaths": {
      "utils": "./src/utils"
    }
  }